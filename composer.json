{
    "name": "gf-verifytx/addon",
    "description": "Gravity Forms VerifyTX integration",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "classmap": ["includes/", "class-gf-verifytx.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "GFVerifyTX\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    }
}
